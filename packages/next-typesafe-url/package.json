{
  "name": "next-typesafe-url",
  "description": "Fully typesafe, JSON serializable, and zod validated URL search params, dynamic route params, and routing for NextJS pages directory",
  "license": "MIT",
  "version": "4.0.1",
  "author": "Ethan Niser",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethanniser/next-typesafe-url"
  },
  "homepage": "https://next-typesafe-url.dev",
  "keywords": [
    "next",
    "nextjs",
    "urlSearchParams",
    "typesafe",
    "typescript",
    "zod"
  ],
  "bin": "./dist/cli.js",
  "types": "./root.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./pages": {
      "require": "./dist/pages.js",
      "import": "./dist/pages.mjs"
    },
    "./app": {
      "require": "./dist/app/index.js",
      "import": "./dist/app/index.mjs"
    },
    "./app/hoc": {
      "require": "./dist/app/hoc.js",
      "import": "./dist/app/hoc.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "pages": [
        "./dist/pages.d.ts"
      ],
      "app": [
        "./dist/app/index.d.ts"
      ],
      "app/hoc": [
        "./dist/app/hoc.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc",
    "dev": "npm run build -- --watch",
    "test:watch": "vitest",
    "test": "vitest run",
    "lint": "eslint src/**",
    "publish:beta": "npm publish --tag beta",
    "clean:dist": "rm ./dist -r",
    "docs:gen": "typedoc --out ./api-docs ./src/index.ts ./src/app/index.ts ./src/pages.ts ./src/app/hoc.tsx --plugin typedoc-plugin-missing-exports"
  },
  "dependencies": {
    "@decs/typeschema": "^0.10.2",
    "chokidar": "^3.5.3",
    "meow": "9.0.0"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.31.8",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.7",
    "eslint": "8.45.0",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "7.1.0",
    "typedoc": "^0.25.0",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typescript": "5.1.6",
    "valibot": "^0.13.1",
    "zod": "^3.20.2"
  },
  "peerDependencies": {
    "next": "^13.4.12",
    "react": "^18.2.0"
  }
}
